<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="463.0" prefWidth="659.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="238.0" layoutY="62.0" text="Wybór Eksperymentu">
         <font>
            <Font name="System Bold" size="18.0" />
         </font></Label>
      <TableView fx:id="experimentTable" layoutX="77.0" layoutY="149.0" onMouseClicked="#tableClicked" prefHeight="200.0" prefWidth="520.0">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="250.0" text="Nazwa" />
          <TableColumn fx:id="drugColumn" prefWidth="268.0" text="Testowany lek" />
        </columns>
      </TableView>
      <Button fx:id="chooseExperimentButton" disable="true" layoutX="531.0" layoutY="394.0" mnemonicParsing="false" onAction="#goToExperiment" text="Zatwierdź" />
      <Label fx:id="chooseExperimentLabel" layoutX="254.0" layoutY="398.0" text="Przejdź do danych wybranego eksperymentu" textFill="#168634">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="loggedAsLabel" layoutX="498.0" layoutY="14.0" prefHeight="50.0" prefWidth="134.0" text="Zalogowano jako :" wrapText="true">
         <font>
            <Font name="Caladea" size="12.0" />
         </font></Label>
   </children>
</AnchorPane>
